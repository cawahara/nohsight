<div id="form-place-vue">
   <div class="md-form">
      <div class="init-area">
         <label>開催地</label><span class="required">必須</span>
         <input type="text" name="place[title]" id="place_title" v-bind:value="values.title">
      </div>
      <div class="error-msgs"><span v-if="typeof error_msgs !== 'undefined'">{{ error_msgs.title }}</span></div>
   </div>
   <div class="md-form">
      <div class="init-area">
         <label>住所</label><span class="required">必須</span>
         <input type="text" name="place[address]" id="place_address" v-bind:value="values.address">
      </div>
      <div class="error-msgs"><span v-if="typeof error_msgs !== 'undefined'">{{ error_msgs.address }}</span></div>
   </div>
   <div class="md-form">
      <div class="init-area">
         <label>サイトURL</label>
         <input type="text" name="place[official_url]" id="place_official_url" v-bind:value="values.official_url">
      </div>
      <div class="error-msgs"></div>
   </div>
</div>
